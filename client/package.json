{
  "name": "upcasebookreview",
  "version": "0.0.0",
  "description": "Upcase Book Review",
  "scripts": {
    "clean:js": "rm -rf ./../public/assets/scripts",
    "clean:css": "rm -rf ./../public/assets/styles",
    "clean": "npm run clean:js && npm run clean:css",
    "compile": "NODE_ENV=production webpack --config ./webpack/webpack.js",
    "build": "npm i && npm run clean && npm run compile",
    "watch": "webpack-dev-server --config ./webpack/webpack.js",
    "start": "npm run watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.9.1"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-1": "^6.5.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "nib": "^1.1.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "private": true
}
